{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ],
      "destination": "/server.js",
      "permanent": false
    }
  ]
}
